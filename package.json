{
   "name": "audio-digest",
   "version": "1.0.0",
   "main": "src/main.ts",
   "scripts": {
      "dev": "npm run killport && nodemon",
      "killport": "npx kill-port 3000",
      "build": "rimraf ./build && tsc -p tsconfig.build.json",
      "start": "ts-node src/index.ts",
      "lint": "eslint . --ext .ts",
      "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
      "test": "jest",
      "prisma:generate": "prisma generate --schema=./src/infra/database/prisma/schema.prisma",
      "prisma:migrate": "prisma migrate dev --schema=./src/infra/database/prisma/schema.prisma --name",
      "prisma:studio": "prisma studio --schema=./src/infra/database/prisma/schema.prisma",
      "typesafe-i18n": "typesafe-i18n"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/express": "^4.17.17",
      "@types/fluent-ffmpeg": "^2.1.21",
      "@types/jest": "^29.5.0",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/twilio": "^3.19.3",
      "@typescript-eslint/eslint-plugin": "^5.57.1",
      "@typescript-eslint/parser": "^5.57.1",
      "eslint": "^8.37.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^29.5.0",
      "nodemon": "^2.0.22",
      "prettier": "^2.8.7",
      "prisma": "^4.13.0",
      "rimraf": "^4.4.1",
      "ts-jest": "^29.1.0",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@prisma/client": "^4.13.0",
      "@types/axios": "^0.14.0",
      "@types/node-fetch": "^2.6.3",
      "@types/uuid": "^9.0.1",
      "axios": "^1.3.6",
      "body-parser": "^1.20.2",
      "express": "^4.18.2",
      "fluent-ffmpeg": "^2.1.2",
      "multer": "^1.4.5-lts.1",
      "openai": "^3.2.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "twilio": "^4.10.0",
      "typesafe-i18n": "^5.24.3",
      "uuid": "^9.0.0"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src"
      ],
      "exec": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/server.ts",
      "ext": "ts, js"
   }
}
